<?xml version="1.0" encoding="utf-8"?>
<com.zhy.android.percent.support.PercentLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/item_click_bg_selector"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <com.zhy.android.percent.support.PercentLinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_heightPercent="12%h"
        android:paddingLeft="10dip"
        android:paddingRight="10dip">
        <com.zhy.android.percent.support.PercentRelativeLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_heightPercent="70%h"
            app:layout_widthPercent="70%h"
            android:layout_gravity="center_vertical">
            <ImageView
                android:id="@+id/item_gzfshy_listview_icon"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@mipmap/morentouxiang"
                android:background="@drawable/item_icon_bg"
                />
            <ImageView
                android:id="@+id/item_gzfshy_listview_hongiang"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_heightPercent="30%h"
                app:layout_widthPercent="30%h"
                android:src="@mipmap/qian"
                android:visibility="invisible"
                />
            <ImageView
                android:id="@+id/item_gzfshy_listview_vip"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_heightPercent="30%h"
                app:layout_widthPercent="30%h"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:src="@mipmap/vip"
                android:visibility="invisible"
                />
        </com.zhy.android.percent.support.PercentRelativeLayout>
        <com.zhy.android.percent.support.PercentLinearLayout
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="9"
            app:layout_heightPercent="75%h"
            android:layout_marginLeft="10dp"
            android:orientation="vertical"
            android:layout_gravity="center_vertical">

            <com.zhy.android.percent.support.PercentLinearLayout
                android:layout_width="match_parent"
                android:layout_height="21dp"
                android:layout_weight="1"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/item_gzfshy_listview_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:maxLength="10"
                    android:singleLine="true"
                    android:textColor="#333333"
                    app:layout_textSizePercent="55%" />

                <ImageView
                    android:id="@+id/item_gzfshy_listview_shiming"
                    android:layout_width="19dp"
                    android:layout_height="0dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="5dip"
                    android:src="@mipmap/renzheng"
                    android:visibility="invisible"
                    app:layout_heightPercent="50%h"
                    app:layout_widthPercent="60%h" />

                <ImageView
                    android:id="@+id/item_gzfshy_listview_online"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="5dip"
                    android:src="@mipmap/lvdian"
                    android:visibility="invisible"
                    app:layout_heightPercent="30%h"
                    app:layout_widthPercent="30%h" />
            </com.zhy.android.percent.support.PercentLinearLayout>
            <com.zhy.android.percent.support.PercentLinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="0.75"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/item_gzfshy_listview_Sex"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="5dp"
                    app:layout_textSizePercent="50%"
                    android:drawableLeft="@mipmap/san"
                    android:drawablePadding="3dp"
                    android:paddingLeft="7dp"
                    android:paddingRight="7dp"
                    android:paddingTop="1dp"
                    android:paddingBottom="1dp"
                    android:textColor="#FFFFFF"
                    android:layout_gravity="center_vertical"
                    />
                <TextView
                    android:id="@+id/item_gzfshy_listview_role"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="5dp"
                    app:layout_textSizePercent="50%"
                    android:paddingLeft="7dp"
                    android:paddingRight="7dp"
                    android:paddingTop="1dp"
                    android:paddingBottom="1dp"
                    android:textColor="#FFFFFF"
                    android:layout_gravity="center_vertical"
                    />
                <com.zhy.android.percent.support.PercentLinearLayout
                    android:id="@+id/item_dynamic_listview_ll_richCount"
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    app:layout_heightPercent="80%h"
                    android:orientation="horizontal"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/item_caifuzhi_bg"
                    android:layout_marginRight="5dp"
                    >
                    <ImageView
                        android:layout_width="wrap_content"
                        app:layout_widthPercent="110%h"
                        android:layout_height="match_parent"
                        android:src="@mipmap/caifuzhi"
                        android:scaleType="fitXY"
                        />
                    <TextView
                        android:id="@+id/item_dynamic_listview_richCount"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:text="111"
                        android:paddingLeft="3dp"
                        android:paddingRight="3dp"
                        android:textColor="#febf3b"
                        app:layout_textSizePercent="62%h"/>
                </com.zhy.android.percent.support.PercentLinearLayout>
                <com.zhy.android.percent.support.PercentLinearLayout
                    android:id="@+id/item_dynamic_listview_ll_beautyCount"
                    android:layout_width="wrap_content"
                    android:layout_height="0dp"
                    app:layout_heightPercent="80%h"
                    android:orientation="horizontal"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/item_meilizhi_bg"
                    >
                    <ImageView
                        android:layout_width="wrap_content"
                        app:layout_widthPercent="110%h"
                        android:layout_height="match_parent"
                        android:src="@mipmap/meilizhi"
                        android:scaleType="fitXY"
                        />
                    <TextView
                        android:id="@+id/item_dynamic_listview_beautyCount"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:text="111"
                        android:textColor="#f56684"
                        android:paddingLeft="3dp"
                        android:paddingRight="3dp"
                        app:layout_textSizePercent="62%h"/>
                </com.zhy.android.percent.support.PercentLinearLayout>
            </com.zhy.android.percent.support.PercentLinearLayout>
            <com.zhy.android.percent.support.PercentLinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="0.75"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/item_gzfshy_listview_city"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="10dp"
                    android:singleLine="true"
                    app:layout_textSizePercent="58%"
                    android:layout_gravity="center_vertical"
                    />
                <TextView
                    android:id="@+id/item_gzfshy_listview_time"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:singleLine="true"
                    app:layout_textSizePercent="58%"
                    android:layout_gravity="center_vertical"
                    />
            </com.zhy.android.percent.support.PercentLinearLayout>
        </com.zhy.android.percent.support.PercentLinearLayout>
        <ImageView
            android:id="@+id/item_gzfshy_listview_present"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_heightPercent="12%w"
            app:layout_widthPercent="12%w"
            android:layout_gravity="center_vertical"
            android:visibility="gone"
            android:layout_marginRight="5dp"
            />
        <TextView
            android:id="@+id/item_gzfshy_listview_presentCount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:textSize="10sp"
            android:visibility="gone"
            android:layout_marginRight="5dp"
            android:textColor="#333333"/>
        <TextView
            android:id="@+id/item_gzfshy_listview_modou"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            app:layout_textSizePercent="3%w"
            android:visibility="gone"
            android:layout_marginRight="5dp"
            android:textColor="#b73acb"/>
        <ImageView
            android:id="@+id/item_gzfshy_listview_flag"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_heightPercent="8%w"
            app:layout_widthPercent="15%w"
            android:visibility="invisible"
            android:src="@mipmap/jiaguanzhu"
            android:layout_gravity="center_vertical"
            />
    </com.zhy.android.percent.support.PercentLinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#f5f5f5"
        app:layout_marginLeftPercent="20%w"
        android:layout_alignParentBottom="true"/>
</com.zhy.android.percent.support.PercentLinearLayout>